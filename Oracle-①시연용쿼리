/* #############EAS-TEST-03###################### */

/*⑤ TB_EDC_SDC2_POINT */
SELECT * FROM TB_EDC_SDC2_POINT a
where a.specification_group_id = 'ACT_DE_발생개수(챔버)_ALL'
--   and     a.process_code in('5860','6047','6175')
   order by a.first_check_ymdhms desc
  ;
delete TB_EDC_SDC2_POINT;
commit;

/*⑥ TB_EDC_sDC2_POINT_DATA */
select * from TB_EDC_sDC2_POINT_DATA a
  where a.specification_group_id = 'ACT_DE_발생개수(챔버)_ALL'
--   and     a.process_code in('5860','6047','6175')
--   order by a.first_check_ymdhms desc
  ;
delete TB_EDC_sDC2_POINT_DATA;
commit;
